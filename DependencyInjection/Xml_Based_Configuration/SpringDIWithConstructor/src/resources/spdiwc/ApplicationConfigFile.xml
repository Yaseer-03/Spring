<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
	<bean class="beans.spdiwc.Address" id="addressProperties">
	<constructor-arg value="California"/><constructor-arg value="USA"/></bean>
	<bean class="beans.spdiwc.Employee" id="firstObject">
	<constructor-arg value="03"/>
	<constructor-arg value="John"/>
	<constructor-arg ref="addressProperties"/>
	</bean>

</beans>