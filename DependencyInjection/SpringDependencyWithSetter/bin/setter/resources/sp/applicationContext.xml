<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean class="setter.beans.sp.Address" id = "addressBean">
    <property name="city" value="Kurnool"/>
    <property name="state" value="Andhra Pradesh"/>
    </bean>
    <bean class="setter.beans.sp.Employee" id="firstObject">
    <property name="id" value="1"/>
    <property name="name" value="ravi"/>
    <property name="address" ref="addressBean"/>
    </bean>

</beans>